{
    "jshint": {
        "options": {
        	"strict": true,
            "browser": true,
            "devel": true,
            "node": true
        },
        "global": [
            "window","console", "gapi", "define", "ENTER_KEY"
        ]
    },
    
	"formatter": {
    	"js": {
			"indent_size": 1,
			"indent_char": "\t"
		}
	},
	
	"ui": {
		"font":"Courier New",
    	"font_size":14,
    	"navigator": true
	},
	
	search: {
		exclude: [
			'**/*.min.js',
		],
		deemphasize: [
			'node_modules/**',
			'bower_components/**'
		]
	}	
}